{
  "name": "sn-community-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "8.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.3.2",
    "css-loader": "0.28.10",
    "dotenv": "5.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.9",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "3.0.6",
    "jest": "22.1.4",
    "money": "^0.2.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.0",
    "promise": "8.0.1",
    "react": "^16.1.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.1.1",
    "react-fontawesome": "^1.6.1",
    "sn-client-auth-google": "^1.0.0",
    "source-map-loader": "^0.2.1",
    "start": "^5.1.0",
    "style-loader": "0.20.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.3.1",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typeface-roboto": "0.0.54",
    "typescript": "~2.6.2",
    "webpack": "3.11.0",
    "url-loader": "1.0.1",
    "webpack-dev-server": "3.1.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "stackoverflow": "node customscripts/stackoverflow.js",
    "issues": "node customscripts/issues.js",
    "pullrequests": "node customscripts/pullrequests.js",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "fs": "0.0.2",
    "json-loader": "^0.5.7",
    "markdown": "^0.5.0",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.19.3",
    "normalizr": "^3.2.4",
    "radium": "^0.22.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "request": "^2.83.0",
    "semver": "^5.4.1",
    "sn-client-js": "^3.0.0",
    "sn-redux": "^3.4.0"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
